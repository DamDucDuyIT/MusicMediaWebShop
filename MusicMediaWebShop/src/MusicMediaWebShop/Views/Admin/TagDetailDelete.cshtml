@model TagDetail

@{
    Layout = "_AdminLayout";
    ViewData["Title"] = "Admin_TagDetailDelete";
}
<div id="page-wrapper">
    <h2>Delete</h2>
    <p class="text-danger">@ViewData["ErrorMessage"]</p>
    <h3>Are you sure you want to delete this?</h3>
    <div>
        <h4>Tag Detail</h4>
        <hr />
        <dl class="dl-horizontal">
            <dt>
                @Html.DisplayNameFor(model => model.TagDetailName)
            </dt>
            <dd>
                @Html.DisplayFor(model => model.TagDetailName)
            </dd>
        </dl>

        <form asp-action="TagDetailDelete">
            <div class="form-actions no-color">
                <input type="submit" value="Delete" class="btn btn-default" /> |
                <a asp-action="TagDetail">Back to List</a>
            </div>
        </form>
    </div>
</div>

