@model IEnumerable<ApplicationUser>
@{
    ViewData["Title"] = "Admin_Order";
    Layout = "_AdminLayout";
}
<div id="page-wrapper">
    <div class="row">
        <div class="col-lg-12">
            <div class="panel panel-default">
                <div class="panel-heading">
                    User
                </div>
                <!-- /.panel-heading -->
                <div class="panel-body">
                    <table width="100%" class="table table-striped table-bordered table-hover" id="dataTables-example">
                        <thead>
                            <tr>
                                <th>User ID</th>
                                <th>User Email</th>
                                <th>User Name</th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var user in Model)
                            {
                                <tr class="odd gradeX">
                                    <td>@user.Id</td>
                                    <td>@user.Email</td>
                                    <td>@user.UserName</td>     
                                </tr>
                            }


                        </tbody>
                    </table>
                    <!-- /.table-responsive -->
                </div>
                <!-- /.panel-body -->
            </div>
            <!-- /.panel -->
        </div>
        <!-- /.col-lg-12 -->
    </div>
</div>